<fo:block-container height="110mm" start-indent="0" end-indent="0" keep-together="always" font-family="Arial, Frutiger, Helvetica, Liberation Sans">
    
    <fo:block-container height="5mm">
    <fo:block text-align="center" font-size="8pt">
        
    </fo:block>
</fo:block-container>

    <fo:table width="210mm" table-layout="fixed" border="" margin-top="0">
        <fo:table-column column-number="1" column-width="62mm" border-right="" />
        <fo:table-column column-number="2" column-width="56mm" />
        <fo:table-column column-number="3" column-width="92mm" />
        <fo:table-body>
        
            <!-- 1st row: titles, receipt information, qr-code -->
            <fo:table-row>
            
                <!-- 1st col: receipt (left) -->
                <fo:table-cell column-number="1" padding="5mm" font-size="8pt">
                      
                    <fo:block-container height="7mm"> 
                        <fo:block font-weight="bold" font-size="11pt">
                            Receipt
                        </fo:block>
                    </fo:block-container>
                    
                    <fo:block-container height="56mm">
                        <fo:block linefeed-treatment="preserve">
                            <fo:block font-weight="bold" font-size="6pt">Account / Payable to</fo:block><fo:block>CH44 3199 9123 0008 8901 2<fo:block>&#xD;</fo:block>Thomas LeClaire<fo:block>&#xD;</fo:block>Rue examplaire 22a<fo:block>&#xD;</fo:block>1000 Lausanne</fo:block>
<fo:block font-weight="bold" font-size="6pt">Reference</fo:block><fo:block>12 34567 89012 34567 89012 34567</fo:block>
<fo:block font-weight="bold" font-size="6pt">Payable by (name/address)</fo:block><img src="data:image/svg+xml;base64," style="width:52mm; height:20mm;" class="qr-bill-placeholder" id="placeholder_payable_by_receipt">
                        </fo:block>
                    </fo:block-container>
                        
                    <fo:block-container height="14mm">
                        <fo:table>
                            <fo:table-body>
                                <fo:table-row>
                                    <fo:table-cell margin="0" width="15mm">
                                        <fo:block><fo:block font-weight="bold" font-size="6pt">Currency</fo:block><fo:block>CHF</fo:block>
</fo:block>
                                    </fo:table-cell>
                                    <fo:table-cell margin="0">
                                        <fo:block font-weight="bold" font-size="6pt">Amount</fo:block><fo:block>25.90</fo:block>

                                    </fo:table-cell>
                                </fo:table-row>
                            </fo:table-body>
                        </fo:table>
                    </fo:block-container>
                    
                    <fo:block-container height="18mm">
                        <fo:block font-weight="bold" font-size="6pt" text-align="right">
                            Acceptance point
                        </fo:block>
                    </fo:block-container>
                </fo:table-cell>
                
                <!-- 2nd col: payment part (middle) -->
                <fo:table-cell column-number="2" padding="5mm 5mm 0 5mm" font-size="10pt">
                
                    <fo:block-container height="7mm">
                        <fo:block font-weight="bold" font-size="11pt">
                            Payment part
                        </fo:block>
                    </fo:block-container>
                    
                    <fo:block-container height="56mm">
                        <fo:block padding-top="5mm" padding-bottom="5mm">
                            <fo:external-graphic src="data:image/png;base64," content-height="46mm" content-width="46mm" />
                        </fo:block>
                    </fo:block-container>
                    
                    <fo:block-container height="22mm">
                        <fo:table>
                            <fo:table-body>
                                <fo:table-row>
                                    <fo:table-cell margin="0" width="20mm">
                                        <fo:block><fo:block font-weight="bold" font-size="8pt">Currency</fo:block><fo:block>CHF</fo:block>
</fo:block>
                                    </fo:table-cell>
                                    <fo:table-cell margin="0">
                                        <fo:block font-weight="bold" font-size="8pt">Amount</fo:block><fo:block>25.90</fo:block>

                                    </fo:table-cell>
                                </fo:table-row>
                            </fo:table-body>
                        </fo:table>
                    </fo:block-container>
                    
                    <!-- Payment part further info (bottom middle to right) -->
                    <fo:block-container height="10mm">
                        <fo:block font-size="7pt">
                            <fo:block></fo:block>

                        </fo:block>
                    </fo:block-container>
                    
                </fo:table-cell>
                
                <!-- 3rd col: payment part info (right) -->
                <fo:table-cell column-number="3">
                    <fo:block-container height="85mm">
                        <fo:block font-size="10pt" linefeed-treatment="preserve" padding-top="1mm">
                            <fo:block font-weight="bold" font-size="8pt">Account / Payable to</fo:block><fo:block>CH44 3199 9123 0008 8901 2<fo:block>&#xD;</fo:block>Thomas LeClaire<fo:block>&#xD;</fo:block>Rue examplaire 22a<fo:block>&#xD;</fo:block>1000 Lausanne</fo:block>
<fo:block font-weight="bold" font-size="8pt">Reference</fo:block><fo:block>12 34567 89012 34567 89012 34567</fo:block>
<fo:block font-weight="bold" font-size="8pt">Additional information</fo:block><fo:block>Invoice 1234568<fo:block>&#xD;</fo:block>Gardening work<fo:block>&#xD;</fo:block>Bill Information</fo:block>
<fo:block font-weight="bold" font-size="8pt">Payable by (name/address)</fo:block><img src="data:image/svg+xml;base64," style="width:65mm; height:25mm;" class="qr-bill-placeholder" id="placeholder_payable_by">
                        </fo:block>
                    </fo:block-container>
                </fo:table-cell>
                
            </fo:table-row>
            
        </fo:table-body>
    </fo:table>
</fo:block-container>